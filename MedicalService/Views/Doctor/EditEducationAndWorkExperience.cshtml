@model MedicalService.ViewModels.DoctorViewModels.EditEducationAndWorkExperience

<h4>Edit Education And Work Experience</h4>
<div class="row">
    <div class="col-md-6">
        <form form asp-action="EditEducationAndWorkExperience" asp-controller="Doctor">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input type="hidden" asp-for="Id" />
            </div>
            <div class="form-group">
                <label asp-for="University">University</label>
                <input asp-for="University" class="form-control"/>

            </div>
            <div class="form-group">
                <label asp-for="WorkExperience">Work Experience (years)</label>
                <input asp-for="WorkExperience" class="form-control"/>
            </div>
            <button id="update-profile-button" type="submit" class="btn btn-outline-success">Save</button>
        </form>
    </div>
</div>